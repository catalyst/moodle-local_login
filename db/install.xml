<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/login/db" VERSION="20220830" COMMENT="XMLDB file for Moodle local/login."
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_login" COMMENT="users redirected to new login url">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="redirected" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="sesskey" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="The sesskey of a session"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for this grade."/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid" UNIQUE="true" FIELDS="userid"/>
        <INDEX NAME="redirected" UNIQUE="false" FIELDS="redirected"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
